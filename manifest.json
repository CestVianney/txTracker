{
    "manifest_version": 3,
    "name": "Blockchain Wallet Tracker",
    "version": "1.0",
    "description": "Track wallet activity on a blockchain network",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
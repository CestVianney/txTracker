{
    "manifest_version": 3,
    "name": "Blockchain Wallet Tracker",
    "version": "1.0",
    "description": "Track wallet activity on a blockchain network",
    "permissions": ["scripting", "windows", "activeTab"], 
    "host_permissions": ["<all_urls>"], 
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "action": {
      "default_icon": "icon.png",
      "default_title": "click !"
    }
  }
  